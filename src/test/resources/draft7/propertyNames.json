[
  {
    "description": "propertyNames validation",
    "schema": {
      "type": "object",
      "properties": {
        "foo": {
          "oneOf": [
            {
              "type": "string",
              "nullable": true
            },
            {
              "type": "array",
              "nullable": true,
              "items": {
                "type": "string"
              }
            }
          ]
        }
      },
      "propertyNames":{ "enum": ["foo"] }
    },
    "tests": [
      {
        "description": "all property names valid",
        "isTypeLoose": true,
        "data": {
          "foo": "test"
        }
      },
      {
        "description": "all property names valid",
        "isTypeLoose": true,
        "data": {
          "foo": ["test"]
        }
      }
    ]
  }
]
